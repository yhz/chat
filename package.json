{
  "name": "chat",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "server:dev": "nodemon --watch 'src/server/**' --watch 'src/shared/**' --ext 'ts' --exec node -r ts-node/register -r tsconfig-paths/register src/server/index.ts",
    "server:dev:inspect": "nodemon --watch 'src/server/**' --watch 'src/shared/**' --ext 'ts' --exec node inspect -r ts-node/register -r tsconfig-paths/register src/server/index.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "ng-inline-svg-2": "^15.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/util.promisify": "^1.0.4",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "~4.7.2"
  }
}
